<form method="get" action="/quizes">
	<p> Buscar por: </p>
	<input type="text" name="search" value=""/>
	<input type="submit" value="Buscar"><p>
</form>
<table>
	<% var i; for (i=0; i < quizes.length; i++) { %>
		<tr>
			<td>
				<a href="quizes/<%= quizes[i].id %>">
					<%= quizes[i].pregunta %>
				</a>
			</td>
		</tr>
	<% } %>
</table>
